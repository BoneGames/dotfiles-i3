BUILD_STARTED=$(tmux ls | grep build-dotfiles.*: | sed -E 's/.*build-dotfiles-([^:]*):.*/\1/')

if [ ! -z "$BUILD_STARTED" ]; then
  echo -n " "
  echo $BUILD_STARTED | timeago
else
  echo -n " "
  docker inspect sabrehagen/desktop-environment --format='{{.Created}}' | xargs date +%s -d | timeago
fi
